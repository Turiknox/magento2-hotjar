{
  "name": "turiknox/module-hotjar",
  "description": "A simple Magento 2 module that will add Hotjar tracking code to your web pages.",
  "version": "1.0.0",
  "license": "MIT",
  "require": {
    "php": "~5.6.5|7.0.2|7.0.4|~7.0.6|~7.1.0",
    "magento/module-backend": "100.1.*",
    "magento/module-cms": "101.0.*",
    "magento/module-ui": "100.1.*",
    "magento/module-store": "100.1.*",
    "magento/framework": "100.1.*"
  },
  "type": "magento2-module",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Turiknox/magento2-hotjar"
    }
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Turiknox\\Hotjar\\": ""
    }
  }
}
